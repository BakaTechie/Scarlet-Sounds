  {
    "time": 68,
    "text": "完全無欠のヴァンパイア",
    "cn": "完美无缺的吸血鬼",
    "interlude": false
  },
  {
    "time": 2919,
    "text": "破壊するその手でブレイクアウト",
    "cn": "用那破坏之手实现突破",
    "interlude": false
  },
  {
    "time": 5446,
    "text": "彼女の人生浴びたらクリア",
    "cn": "她的人生若被洗礼则变得清晰",
    "interlude": false
  },
  {
    "time": 8736,
    "text": "強さで心奪う君は",
    "cn": "以力量夺心的你",
    "interlude": false
  },
  {
    "time": 11460,
    "text": "悪魔的な妹様",
    "cn": "恶魔般的妹妹大人",
    "interlude": false
  },
  {
    "time": 16716,
    "text": "ああ何年地下に閉じこもって",
    "cn": "啊啊，多少年被封闭在地下",
    "interlude": false
  },
  {
    "time": 19964,
    "text": "独り過去を刻んでいたの?",
    "cn": "独自铭刻着过去？",
    "interlude": false
  },
  {
    "time": 22456,
    "text": "ああ好きな血液型はわからない?",
    "cn": "啊啊，喜欢血型是什么？",
    "interlude": false
  },
  {
    "time": 25698,
    "text": "ミステリアスなシスターヴァンプ",
    "cn": "神秘莫测的姐妹吸血鬼",
    "interlude": false
  },
  {
    "time": 28622,
    "text": "常に飄々と だけど爛漫で",
    "cn": "总是悠然自得，却又华丽",
    "interlude": false
  },
  {
    "time": 31447,
    "text": "禁忌は甘い果実味",
    "cn": "禁忌是那甘甜的果实味",
    "interlude": false
  },
  {
    "time": 34062,
    "text": "ああボムもないないない",
    "cn": "啊啊，没有爆炸",
    "interlude": false
  },
  {
    "time": 35841,
    "text": "残機ないないない",
    "cn": "没有残机",
    "interlude": false
  },
  {
    "time": 37319,
    "text": "1位になった感想は?",
    "cn": "成为第一名的感受是？",
    "interlude": false
  },
  {
    "time": 39951,
    "text": "「誰かの\u003c好き\u003eになれるなんてよく分からなくて」",
    "cn": "“能够成为谁的‘喜欢’，我真不明白”",
    "interlude": false
  },
  {
    "time": 45583,
    "text": "興味があるかわからない",
    "cn": "是否有兴趣，我也不清楚",
    "interlude": false
  },
  {
    "time": 48869,
    "text": "そんな言葉に また抱え落ちる",
    "cn": "就这样再次被那些话语压垮",
    "interlude": false
  },
  {
    "time": 52650,
    "text": "また推しにさせる",
    "cn": "再次被推上前台",
    "interlude": false
  },
  {
    "time": 54496,
    "text": "誰もが一度は好きになる",
    "cn": "无论是谁，都会有喜欢的人",
    "interlude": false
  },
  {
    "time": 57500,
    "text": "最強妹フランドール",
    "cn": "最强的妹妹芙兰朵露",
    "interlude": false
  },
  {
    "time": 60434,
    "text": "他の作品じゃ味わえない",
    "cn": "在其他作品中无法体验",
    "interlude": false
  },
  {
    "time": 63342,
    "text": "神が生み落とした伝説",
    "cn": "神所创造的传说",
    "interlude": false
  },
  {
    "time": 65941,
    "text": "ああそのスリルな弾幕",
    "cn": "啊啊，那刺激的弹幕",
    "interlude": false
  },
  {
    "time": 68732,
    "text": "誰も彼もが虜になってゆく",
    "cn": "无论是谁都会被俘虏",
    "interlude": false
  },
  {
    "time": 72327,
    "text": "紅い瞳もセリフも",
    "cn": "红色的眼睛和台词",
    "interlude": false
  },
  {
    "time": 74533,
    "text": "織り成すその全て",
    "cn": "构成她的一切",
    "interlude": false
  },
  {
    "time": 76189,
    "text": "フランドール",
    "cn": "芙兰朵露",
    "interlude": false
  },
  {
    "time": 78128,
    "text": "はいはい私は悪い子です",
    "cn": "是的是的，我是坏孩子",
    "interlude": false
  },
  {
    "time": 81027,
    "text": "だから幽閉されてるんです",
    "cn": "因此被幽禁",
    "interlude": false
  },
  {
    "time": 83567,
    "text": "お姉様のおまけの妹です",
    "cn": "作为姐姐的附属品妹妹",
    "interlude": false
  },
  {
    "time": 86767,
    "text": "全て言いなりで終わらせたくない",
    "cn": "我不想一切都听从命令结束",
    "interlude": false
  },
  {
    "time": 89726,
    "text": "ここの部屋の鍵壊せない訳ない",
    "cn": "这个房间的钥匙不可能破坏不了",
    "interlude": false
  },
  {
    "time": 92220,
    "text": "だけど壊さないだって上手く使えない",
    "cn": "但是不破坏的话，也用不好",
    "interlude": false
  },
  {
    "time": 95140,
    "text": "籠の鳥の私なんて許せはしない",
    "cn": "我这样的笼中鸟，不可能被原谅",
    "interlude": false
  },
  {
    "time": 97977,
    "text": "鏡に映らない自分のこと許せない",
    "cn": "无法接受镜中映不出的自己",
    "interlude": false
  },
  {
    "time": 100854,
    "text": "誰もが一度は好きになる",
    "cn": "无论是谁，都会有喜欢的人",
    "interlude": false
  },
  {
    "time": 103822,
    "text": "真紅の妹フランドール",
    "cn": "真红的妹妹芙兰朵露",
    "interlude": false
  },
  {
    "time": 106754,
    "text": "弱点は流れる水と",
    "cn": "真红的妹妹芙兰朵露",
    "interlude": false
  },
  {
    "time": 109559,
    "text": "熱く輝いてる太陽",
    "cn": "炽热闪耀的太阳",
    "interlude": false
  },
  {
    "time": 112610,
    "text": "ああ弱いとこだって魅力で",
    "cn": "啊啊，弱点也是魅力",
    "interlude": false
  },
  {
    "time": 115880,
    "text": "それぞれの解釈で広がった",
    "cn": "各自的解释让它扩展",
    "interlude": false
  },
  {
    "time": 118584,
    "text": "あなたの幻想郷",
    "cn": "你的幻想乡",
    "interlude": false
  },
  {
    "time": 120744,
    "text": "そこに居るあなたのフランドール",
    "cn": "在那里的你的芙兰朵露",
    "interlude": false
  },
  {
    "time": 124306,
    "text": "天使の笑顔のヴァンパイア",
    "cn": "拥有天使笑容的吸血鬼",
    "interlude": false
  },
  {
    "time": 127617,
    "text": "コンティニューできないのここは",
    "cn": "无法继续的这里",
    "interlude": false
  },
  {
    "time": 130419,
    "text": "レーヴァテインは神話のレプリカ",
    "cn": "雷瓦汀是神话的复制品",
    "interlude": false
  },
  {
    "time": 133964,
    "text": "迷路の出口こそが愛だ",
    "cn": "迷宫的出口才是爱",
    "interlude": false
  },
  {
    "time": 137154,
    "text": "紅い霧で隠すレミリア",
    "cn": "隐藏在红雾中的蕾米莉亚",
    "interlude": false
  },
  {
    "time": 140423,
    "text": "完璧にこなすのは咲夜",
    "cn": "完美处理一切的是咲夜",
    "interlude": false
  },
  {
    "time": 143290,
    "text": "七色の羽根の私がイデア",
    "cn": "拥有七色翅膀的我是理念",
    "interlude": false
  },
  {
    "time": 146818,
    "text": "弾幕はとびきりの愛だ",
    "cn": "弹幕是无与伦比的爱",
    "interlude": false
  },
  {
    "time": 150857,
    "text": "これまで愛されたことも",
    "cn": "至今为止，我以为没有被爱过",
    "interlude": false
  },
  {
    "time": 153864,
    "text": "愛せたこともないと思ってた",
    "cn": "也没有爱过别人",
    "interlude": false
  },
  {
    "time": 157480,
    "text": "そんな私があなたの押しになれたのは",
    "cn": "这样的我能成为你的推手",
    "interlude": false
  },
  {
    "time": 161512,
    "text": "本当だから",
    "cn": "是因为这是真的",
    "interlude": false
  },
  {
    "time": 163460,
    "text": "ありがとう",
    "cn": "谢谢你",
    "interlude": false
  },
  {
    "time": 164576,
    "text": "これからもきっと狂わせていく",
    "cn": "今后也一定会继续疯狂",
    "interlude": false
  },
  {
    "time": 167901,
    "text": "私欲張りフランドール",
    "cn": "自私的芙兰朵露",
    "interlude": false
  },
  {
    "time": 170631,
    "text": "この居場所でみんなのこと",
    "cn": "在这个居所，想要继续",
    "interlude": false
  },
  {
    "time": 173439,
    "text": "愛し壊し続けたいから",
    "cn": "爱着大家，直到破坏为止",
    "interlude": false
  },
  {
    "time": 176157,
    "text": "ああ今日もスペルで勝負",
    "cn": "啊啊，今天也要用法术决一胜负",
    "interlude": false
  },
  {
    "time": 178947,
    "text": "未熟だけれど世界も救ったよ",
    "cn": "虽然不成熟，但也拯救了世界",
    "interlude": false
  },
  {
    "time": 182543,
    "text": "それでもまだ届かなくて",
    "cn": "尽管如此，还是无法传达",
    "interlude": false
  },
  {
    "time": 185190,
    "text": "伝えられずにいたけど",
    "cn": "至今无法表达的",
    "interlude": false
  },
  {
    "time": 188038,
    "text": "今ならもう言えるかも",
    "cn": "但现在也许可以说了",
    "interlude": false
  },
  {
    "time": 190478,
    "text": "これからもずっと愛してる",
    "cn": "今后也会一直爱着你",
    "interlude": false
  }
]